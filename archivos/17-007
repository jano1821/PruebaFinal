+    public Date getFechaProceso() {
+        return fechaProceso;
+    }
+
+    public void setFechaProceso(Date fechaProceso) {
+        this.fechaProceso = fechaProceso;
+    }
+
+    @ManyToOne(cascade = {}, fetch = FetchType.LAZY)
