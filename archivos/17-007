+                                                                                idEtapa,
+                                                                                beanParametrosAuditoria);
+        setRequestAttribute(servletParams, "beanWFLegal", beanWFLegal);
+        setRequestAttribute(servletParams, "beanWFDetalleComentario", beanWFDetalleComentario);
