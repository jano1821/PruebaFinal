+                <input type="hidden" name="strComportamiento" id="strComportamiento" value="<c:out value="${requestScope.strComportamiento}"/>">
+            </td>
+        </tr>
+        <tr>
+          <td>
+            <table border="2" cellpadding="0" cellspacing="0" width="100%" align="center" class="frame">
