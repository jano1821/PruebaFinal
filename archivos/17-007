+    	  var url = path + '/CR_C_CuentaCredito_Servlet' + params;
+    	  document.ifrmDetalleInterno.location.replace(url);
+      }
+
+      function cargarDetalleExterno(){
+          var codigoPersona = byID('codigoPersona').value;
+          var tipoOperacion = 'E';
+          var params = '?action=MOSTRAR_DETALLE';
