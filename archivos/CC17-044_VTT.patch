+public class CR_T_MotivosCambioCondicion_Servlet extends GE_BaseMethodInvocation_Servlet {
+
+    private static final long serialVersionUID = -2436123177639297887L;
+    private CRCambioCondicionesFacadeInterfaceLocal crCambioCondicionesFacadeInterfaceLocal;
+
+    protected void initEJBs() throws Exception {
+        crCambioCondicionesFacadeInterfaceLocal = lookup(CRCambioCondicionesFacade.LocalJNDIName);
+    }
+
+    @Override
+    protected void execute(final ServletParams servletParams) throws Exception {
+    }
+
+    @RequestMapping(value = "VALIDA_FECHA_VENCIMIENTO", responseType = ResponseType.XML)
+    protected void validaFechaVencimientoHandler(final ServletParams servletParams) throws Exception {
+        final BeanParametrosAuditoria beanParametrosAuditoria = getBeanParametrosAuditoria(servletParams);
+        final BeanCRFechaVencimiento bean = mapearBean(servletParams, BeanCRFechaVencimiento.class);
+        crCambioCondicionesFacadeInterfaceLocal.validaFechaVencimientoByMotivoCambioCondicion(bean,
+                                                                                              beanParametrosAuditoria);
+        final StringBuilder sbRespuesta = new StringBuilder();
+        sbRespuesta.append("<codigoError>" + NO_ERROR + "</codigoError>");
+        sbRespuesta.append("<indicadorValidacion>" + bean.getIndicadorValidacion() + "</indicadorValidacion>");
+        responseXML(servletParams, sbRespuesta.toString());
+    }
+
+    @RequestMapping(value = "ACTUALIZA_FECHA_PRIMER_PAGO", responseType = ResponseType.XML)
+    protected void actualizaFechaPrimerPagoHandler(final ServletParams servletParams) throws Exception {
+        final BeanParametrosAuditoria beanParametrosAuditoria = getBeanParametrosAuditoria(servletParams);
+        final BeanCRFechaVencimiento bean = mapearBean(servletParams, BeanCRFechaVencimiento.class);
+        crCambioCondicionesFacadeInterfaceLocal.actualizaFechaPrimerPagoByMotivoCambioCondicion(bean,
+                                                                                                beanParametrosAuditoria);
+        final StringBuilder sbRespuesta = new StringBuilder();
+        sbRespuesta.append("<codigoError>" + NO_ERROR + "</codigoError>");
+        sbRespuesta.append("<fechaPrimerPago>" + financ.util.FormatoFecha.dateToStringDDMMYYYYY(bean.getFechaPrimerPago()) + "</fechaPrimerPago>");
+        responseXML(servletParams, sbRespuesta.toString());
+    }
+
+}
