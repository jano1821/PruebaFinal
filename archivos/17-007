+        BeanParametrosAuditoria beanParametrosAuditoria = getBeanParametrosAuditoria(servletParams);
+        String idMaestra = getParameter(servletParams, "idMaestra");
+        String idEtapa = getParameter(servletParams, "idEtapa");
