+    public WFMaestra getwfMaestra() {
+        return wfMaestra;
+    }
+
+    public void setwfMaestra(WFMaestra wfMaestra) {
+        this.wfMaestra = wfMaestra;
+    }
+
+    @ManyToOne(cascade = {}, fetch = FetchType.LAZY)
+    @JoinColumn(name = "C_IDETAP", unique = false)
