+	                          </c:when>
+	                          <c:otherwise>
+								<delfinTagHtml:boton id="${beanWFEvento.idEvento}" nombre="${beanWFEvento.nombreBoton}" onclick="javascript:funcionGenerica('${beanWFEvento.nombrePrograma}')" />
+	                          </c:otherwise>
