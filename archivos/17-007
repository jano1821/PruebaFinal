+            var seleccionadosSolicitudes = "";
+            for(var i = 0; i < NumerodeFilas(); i++){
+                if(myData[i][0] == "Si"){
+                	seleccionadosSolicitudes += myData[i][1] + "-";
+                }
+            }
+            return seleccionadosSolicitudes;
+        }
+
